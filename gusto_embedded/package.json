{
  "name": "@gusto/embedded-api",
  "version": "0.11.0",
  "author": "Speakeasy",
  "type": "module",
  "main": "./esm/index.js",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./esm/index.d.ts",
      "default": "./esm/index.js"
    },
    "./package.json": "./package.json",
    "./*.js": {
      "source": "./src/*.ts",
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    },
    "./*": {
      "source": "./src/*.ts",
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Gusto/gusto-typescript-client.git",
    "directory": "gusto_embedded"
  },
  "scripts": {
    "lint": "eslint --cache --max-warnings=0 src",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {"optional":true},
    "react": {"optional":true},
    "react-dom": {"optional":true}
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@tanstack/react-query": "^5.61.4",
    "@types/react": "^18.3.12",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.26.0"
  },
  "dependencies": {
    "zod": "^3.25.0 || ^4.0.0"
  }
}
